{
  "name": "my-app",
  "version": "0.0.1",
  "scripts": {
    "dev": "npx vite dev",
    "build": "npx vite build",
    "preview": "npx vite preview",
    "test": "npx playwright test",
    "check": "npx svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "npx svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "npx vitest",
    "lint": "npx prettier --plugin-search-dir . --check . && eslint .",
    "format": "npx prettier --plugin-search-dir . --write .",
    "prepare": "npx npm run smui-theme",
    "smui-theme": "npx smui-theme compile static/smui.css -i src/theme"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@neoconfetti/svelte": "^1.0.0",
    "@playwright/test": "^1.28.1",
    "@smui/button": "^7.0.0-beta.8",
    "@smui/card": "^7.0.0-beta.8",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@types/cookie": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.26.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "smui-theme": "^7.0.0-beta.8",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.3.0",
    "vitest": "^0.25.3",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "@faker-js/faker": "^8.0.1",
    "@smui/data-table": "^7.0.0-beta.8",
    "@smui/icon-button": "^7.0.0-beta.8"
  },
  "type": "module"
}
