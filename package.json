{
	"name": "frontend",
	"version": "0.0.1",
	"scripts": {
		"dev": "npx vite dev",
		"build": "npx vite build",
		"preview": "npx vite preview",
		"test": "npx playwright test",
		"test:unit": "npx vitest",
		"lint": "npx prettier --plugin-search-dir . --check . && eslint .",
		"format": "npx prettier --plugin-search-dir . --write .",
		"check": "npx svelte-kit sync && npx svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "npx svelte-kit sync && npx svelte-check --tsconfig ./tsconfig.json --watch",
		"prepare": "npm run smui-theme",
		"smui-theme": "npx smui-theme compile static/smui.css -i src/theme"
	},
	"type": "module",
	"devDependencies": {
		"@faker-js/faker": "^8.0.2",
		"@fontsource/fira-mono": "^5.0.2",
		"@neoconfetti/svelte": "^1.0.0",
		"@playwright/test": "^1.34.3",
		"@smui/button": "^7.0.0-beta.8",
		"@smui/card": "^7.0.0-beta.8",
		"@smui/data-table": "^7.0.0-beta.8",
		"@smui/fab": "^7.0.0-beta.8",
		"@smui/icon-button": "^7.0.0-beta.8",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.20.0",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.29.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"smui-theme": "^7.0.0-beta.8",
		"stylelint": "^15.10.1",
		"stylelint-config-standard": "^33.0.0",
		"svelte": "^3.59.1",
		"svelte-adapter-github": "^1.0.0-next.0",
		"svelte-check": "^3.4.3",
		"svelte-material-ui": "^7.0.0-beta.8",
		"svelte-time": "^0.7.1",
		"tslib": "^2.5.2",
		"typescript": "^5.0.4",
		"vite": "^4.3.9",
		"vitest": "^0.31.2"
	}
}
